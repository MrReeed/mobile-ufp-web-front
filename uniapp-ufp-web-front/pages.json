{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "消防安全"
			}
		}, {
			"path": "pages/map/map",
			"style": {
				"app-plus": {
					"titleNView": false
				}
			}

		},{
			"path": "pages/maintenance/maintenance",
			"style": {
				"app-plus": {
					"titleNView": false
				}
			}

		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"selectedColor": "#4e9bff",
		"color": "#000",
		"list": [{
				"text": "首页",
				"pagePath": "pages/index/index",
				"iconPath": "static/icon/home.png",
				"selectedIconPath": "static/icon/home-fill.png"
			},
			{
				"text": "地图",
				"pagePath": "pages/map/map",
				"iconPath": "static/icon/location.png",
				"selectedIconPath": "static/icon/location-fill.png"
			},
			{
				"text": "维保",
				"pagePath": "pages/maintenance/maintenance",
				"iconPath": "static/icon/wrench.png",
				"selectedIconPath": "static/icon/wrench-fill.png"
			},
			{
				"text": "我的",
				"pagePath": "pages/user/user",
				"iconPath": "static/icon/user.png",
				"selectedIconPath": "static/icon/user-fill.png"
			}
		]
	}
}
